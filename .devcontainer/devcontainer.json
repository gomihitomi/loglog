{
  "name": "DevContainer-Node21",
  "dockerComposeFile": "compose.yml",

  "service": "loglog",
  "remoteUser": "node",

  "workspaceFolder": "/workspace",
  
  "customizations": {
    "vscode": {
      "extensions": [
        "eamodio.gitlens",
        "mhutchie.git-graph",
        "bradlc.vscode-tailwindcss",
        "esbenp.prettier-vscode",
        "dbaeumer.vscode-eslint",
        "usernamehw.errorlens"
      ]
    }
  }

